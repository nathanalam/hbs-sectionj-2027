            <div class="text-center p-4 border border-gray-200 rounded-lg shadow-md">
                <img
                    src="/assets/hbs_classcard_photos/dimitri_henry.jpg"
                    class="w-32 h-32 rounded-full mx-auto mb-4 border-4 border-white shadow-lg"
                    alt="Profile Photo"
                />
                <h3 class="text-xl font-semibold text-gray-900">Dimitri Henry</h3>
                <p class="text-hbs-crimson font-medium">Section President</p>
                <p class="text-sm text-gray-600 mt-2">HBS/HKS!</p>
            </div>

            <!-- Officer Card 2 -->
